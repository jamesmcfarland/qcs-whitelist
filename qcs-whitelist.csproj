<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>qcs_whitelist</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Apis.Sheets.v4" Version="1.55.0.2371" />
  </ItemGroup>

  <ItemGroup >
    <None Update="credentials.json" CopyToOutputDirectory="PreserveNewest" />
  </ItemGroup>
  <ItemGroup >
    <None Update="spreadsheets.txt" CopyToOutputDirectory="PreserveNewest" />
  </ItemGroup>


</Project>
